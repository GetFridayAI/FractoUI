{
  "name": "@featured.pixels/fracto-ui",
  "version": "0.2.0",
  "description": "Re-usable UI elements for FridayAI apps",
  "keywords": [
    "ui",
    "elements",
    "components"
  ],
  "author": "Featured Pixels",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GetFridayAI/FractoUI.git"
  },
  "bugs": {
    "url": "https://github.com/GetFridayAI/FractoUI/issues"
  },
  "homepage": "https://github.com/GetFridayAI/FractoUI#readme",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles/**/*.less"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "build:less": "cp -R src/styles dist/styles",
    "preview": "vite preview",
    "test": "jest src/tests",
    "prepublish": "npm run build && npm run build:types && npm run build:less"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.21.1",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "isNpmModule": true
}
